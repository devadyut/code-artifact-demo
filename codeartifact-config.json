{
  "aws": {
    "region": "eu-west-1",
    "accountId": "${AWS_ACCOUNT_ID}",
    "profile": "default"
  },
  "codeartifact": {
    "domain": "myorg",
    "repository": "myorg-repo",
    "scope": "@myorg",
    "externalConnection": "public:npmjs"
  },
  "github": {
    "repository": "devadyut/code-artifact-demo"
  },
  "paths": {
    "sharedLibs": "./my-lib",
    "consumers": ["./my-api/auth", "./my-api/core", "./my-ui"]
  }
}
